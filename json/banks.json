          "pattern" : "^(POPOLN)\\sOFFLINE\\s(.*?),\\s[A-Z]{3}\\s([\\d\\.]+\\s[A-Z]{3})\\sCARD\\*(\\d{4}).*?\\sBAL\\s([\\d\\.]+\\s[A-Z]{3})\\s([\\d\\.]+\\s[\\d:]+).*",
