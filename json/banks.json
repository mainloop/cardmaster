          "pattern" : "^(SPISANIE).*?#\\*(\\d{5})\\sBAL\\s([\\d\\.]+\\s[A-Z]{3})\\s([\\d\\.]+\\s[\\d:]+).*",
          "groups": [  "OPERATION","CARD", "BALANCE", "DATE" ]
